<% unless Cartodb.config[:google_analytics_ua].blank? %>
    <script type="text/javascript">
      var _gaq = _gaq || []; 
      _gaq.push(['_setAccount', '<%= Cartodb.config[:google_analytics_ua] %>']);
      _gaq.push(['_setDomainName', 'cartodb.com']);
      _gaq.push(['_setAllowLinker', true]);
      _gaq.push(['_trackPageview']);

      _gaq.push(['_setCustomVar', 1, 'Member Type', '<%= stringified_member_type %>', 1]);

      (function() { var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true; ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js'; var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s); })();
    </script>
<% end %>
