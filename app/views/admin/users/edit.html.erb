<% content_for(:page_title) do %>
  Edit user information |
<% end %>
<%= content_for(:js) do %>
  <%= javascript_include_tag :cdb, :application %>
  <script type="text/javascript">
    var username = "<%= current_user.username %>"
      , type = "oauth"
      , config = <%=raw frontend_config.html_safe %>;
  </script>
<% end %>
<%= content_for(:css) do %>
  <%= stylesheet_link_tag :common, :organization, :media => 'all' %>
<% end %>

<article>
  <div class="inner">
    <section class="left block long">
      <%= render partial: 'form' %>
    </section>
    <aside class="right">
      <%= render partial: 'other_users' %>
    </aside>
  </div>
</article>
