  <% content_for(:page_title) do %>
    <%= @table.name %> |
  <% end %>

  <% content_for(:css) do %>
    <%= stylesheet_link_tag :common, :table, :map, :media => 'all' %>
  <% end %>

  <script>
    var table_name = '<%= @table.name %>'
      , table_id = '<%= @table.id %>'
      , user_data = <%=raw @table.owner.to_json %>
      , map = <%=raw @table.map.to_json %>
      , layers = [
       <%=raw @table.map.layers.map { |layer| layer.values.to_json }.join(',') %>
       ]
       , config = JSON.parse('<%=raw frontend_config %>');


  </script>

  <% content_for(:js) do %>
    <%= javascript_include_tag :application, :cdb, :table_public %>
  <% end %>

  <div class="globalerror"></div>
  <div class="panes"></div>
