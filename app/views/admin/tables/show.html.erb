  <% content_for(:page_title) do %>
    <%= @table.name %> |
  <% end %>

  <% content_for(:css) do %>
    <%= stylesheet_link_tag :common, :table, :map, :media => 'all' %>
  <% end %>


  <script>
    var table_name = '<%= @table.name %>'
      , table_id = '<%= @table.id %>'
