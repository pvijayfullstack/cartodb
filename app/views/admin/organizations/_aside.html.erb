  <div class="head">
    <h3>Your quota</h3>
  </div>
  <div class="content">
    <ul>
      <li>
        <p><%= "#{current_user.organization.users.count} of #{current_user.organization.seats}" %> users in your account.</p>
      </li>
      <li class="margin35">
        <p><%= number_to_human_size current_user.organization.db_size_in_bytes %>
        of 
        <%= number_to_human_size current_user.organization.quota_in_bytes %> 
        used in your account 
        <%= number_to_human_size(current_user.organization.unassigned_quota) %>
        unassigned.</p>
      </li>
      <li class="margin35">
      </li>
    </ul>
  </div>
