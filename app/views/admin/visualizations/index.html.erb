<% content_for(:page_title) do %>
  <%= current_user.username %> |
<% end %>

<%= content_for(:js) do %>
  <%= javascript_include_tag :application, :cdb, :dashboard %>
  <script type="text/javascript">
    var username = "<%= current_user.username %>"
    , user_data = <%=raw current_user.data.to_json.html_safe %>
    , config = <%=raw frontend_config.html_safe %>
    , upgrade_url = "<%= upgrade_url %>";
  </script>
<% end %>
<%= content_for(:css) do %>
  <%= stylesheet_link_tag :common, :dashboard, :media => 'all' %>
<% end %>

<%= render 'admin/shared/subheader' %>
<%= render 'admin/shared/search' %>
<%= render 'admin/visualizations/tables' %>

<article class="support">
<div class="inner">
  <section class="left">
  <h3>As a paying customer you have access to our dedicated support.</h3>
  <p>We encourage our users to participate on our <a href="https://groups.google.com/d/forum/cartodb" target="_blank">community forums</a> and share their knowledge.</p>
  </section>
  <div class="right">
    <a href="mailto:support@cartodb.com" class="button grey">Contact us</a>
  </div>
</div>
</article>

<div class="drag_mamufas">
  <span class="file"></span>
  <p class="bigger strong">Drop your file onto the browser window to create a new table</p>
  <div class="mask"></div>
</div>

<article class="more_data">
<div class="inner">   
  <section class="left">
  <h3><span class="type table">T</span> Looking for more data?</h3>
  <p>Import one of these or browse through our <a href="/dashboard/common_data">common data</a> section</p>
  <ul>
    <li><a class="population import_example" href="http://cartodb.s3.amazonaws.com/static/TM_WORLD_BORDERS_SIMPL-0.3.zip">Start with the country with population numbers</a></li>
    <li><a class="population import_example" href="http://cartodb.s3.amazonaws.com/static/TM_WORLD_BORDERS_SIMPL-0.3.zip">Start with the country with population numbers</a></li>
    <li><a class="population import_example" href="http://cartodb.s3.amazonaws.com/static/TM_WORLD_BORDERS_SIMPL-0.3.zip">Start with the country with population numbers</a></li>
  </ul>
  </section>
</div>
</article>
