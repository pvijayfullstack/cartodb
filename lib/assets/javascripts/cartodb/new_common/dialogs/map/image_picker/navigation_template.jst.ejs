<span class="Filters-separator"></span>
<div class="Filters-row">
  <div class="Filters-type">
    <ul class="Filters-list">
      <% if (kind == 'marker') { %>
      <li class="Filters-typeItem">
        <button data-type="simple_icons" class="Filters-typeLink js-item<%= listing === 'simple_icons' ? ' is-selected' : '' %>">
          Simple icons
        </button>
      </li>
      <li class="Filters-typeItem">
        <button data-type="pin_icons" class="Filters-typeLink js-item<%= listing === 'pin_icons' ? ' is-selected' : '' %>">
          Pin icons
        </button>
      </li>
      <li class="Filters-typeItem">
        <button data-type="maki_icons" class="Filters-typeLink js-item<%= listing === 'maki_icons' ? ' is-selected' : '' %>">
          Maki icons
        </button>
      </li>
      <% } %>
    </ul>
  </div>
  <div class="Filters-order">
    <ul class="Filters-list">
      <li class="Filters-typeItem">
        <button data-type="your_icons" class="Filters-typeLink is-disabled js-your-icons<%= listing === 'your_icons' ? ' is-selected' : '' %>">
          Your icons
        </button>
      </li>
      <li class="Filters-typeItem">
        <button data-type="upload_file" class="Filters-typeLink js-item<%= listing === 'upload_file' ? ' is-selected' : '' %>">
          Upload file
        </button>
      </li>
      <li class="Filters-typeItem">
        <button data-type="dropbox" class="Filters-typeLink js-item<%= listing === 'dropbox' ? ' is-selected' : '' %>">
          Dropbox
        </button>
      </li>
    </ul>
  </div>
</div>
