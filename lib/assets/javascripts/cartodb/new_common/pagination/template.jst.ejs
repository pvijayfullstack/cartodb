<span class="DefaultDescription Pagination-label">
  Page <%= m.get('page') %> of <%= m.pagesCount() %>
</span>
<ul class="Pagination-list">
  <% m.pagesToDisplay().forEach(function(page) { %>
  <li class="Pagination-listItem <%= m.isCurrentPage(page) ? 'is--current' : '' %>">
    <a href="#"><%= page %></a>
  </li>
  <% }) %>
</ul>
