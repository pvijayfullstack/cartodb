<form class="Form js-form">
  <div class="Form-row">
    <div class="Form-rowLabel">
      <label class="Form-label">Enter a URL</label>
    </div>
    <div class="Form-rowData Form-rowData--longer">
      <input type="text" class="Form-input Form-input--longer has-icon js-textInput" value="" placeholder="http://www.cartodb.com/library" />
      <i class="iconFont iconFont-Dribbble Form-inputIcon"></i>
      <div class="Form-inputError">Error. Your URL doesnâ€™t look correct.</div>
    </div>  
  </div>
  <% if (fileEnabled) { %>
    <div class="Form-separator Form-separator--or">or</div>
    <div class="Form-row">
      <div class="Form-rowLabel">
        <label class="Form-label">Upload a file</label>
      </div>
      <div class="Form-rowData Form-rowData--longer">
        <span class="Form-file">
          <input type="file" class="js-fileInput" value=""  />
          <span class="Button Button--main js-fileButton">select a file</span>
        </span>
        <label class="Form-fileLabel js-fileLabel">Like CSV, XLS, ZIP, KML, GPX, <a href="http://developers.cartodb.com/documentation/using-cartodb.html#formats_accepted">see all formats</a></label>
        <label class="Form-fileLabel Form-fileLabel--error js-fileError"></label>
      </div>  
    </div>
  <% } %>
</form>