<a class="OrganizationList-userLink" href="<%- url %>" data-title="<%- sizeOnBytes %> of <%- quotaInBytes %> used">
  <div class="OrganizationList-userAvatar UserAvatar">
    <img src="<%- avatarUrl %>" alt="<%- username %>" src="<%- username %>" class="UserAvatar-img UserAvatar-img--medium-large" />
  </div>
  <div class="OrganizationList-userInfo">
    <div class="OrganizationList-userInfoName">
      <h3 class="OrganizationList-userInfoTitle u-ellipsLongText" title="<%- username %>">
        <%- username %>
        <% if (isOwner) { %>
          <span class="OrganizationList-userInfoLabel">OWNER</span>
        <% } %>
      </h3>
      <h2 class="OrganizationList-userInfoSubtitle u-ellipsLongText" title="<%- user_email %>"><%- user_email %></h2>
    </div>
  </div>
</a>