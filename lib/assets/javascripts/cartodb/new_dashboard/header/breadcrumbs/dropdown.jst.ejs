<ul class="BreadcrumbsDropdown">
  <li>
    <p class="BreadcrumbsDropdown-item">
      <a href="<%= mapsUrl %>" class="<%= !isDatasets && !isLocked ? 'SelectedLink' : '' %>">Your maps</a>
    </p>
    <p class="BreadcrumbsDropdown-item">
      <a href="<%= datasetsUrl %>" class="<%= isDatasets && !isLocked ? 'SelectedLink' : '' %>">Your datasets</a>
    </p>
  </li>
  <li>
    <p class="BreadcrumbsDropdown-sameline BreadcrumbsDropdown-item">
      <a href="<%= lockedDatasetsUrl %>" class="<%= isDatasets && isLocked ? 'SelectedLink' : '' %>">Your locked datasets</a>
      <i class="iconFont iconFont-Lock BreadcrumbsDropdown-lockIcon"></i>
    </p>
    <p class="BreadcrumbsDropdown-sameline BreadcrumbsDropdown-item">
      <a href="<%= lockedMapsUrl %>" class="<%= !isDatasets && isLocked ? 'SelectedLink' : '' %>">Your locked maps</a>
      <i class="iconFont iconFont-Lock BreadcrumbsDropdown-lockIcon"></i>
    </p>
  </li>
</ul
