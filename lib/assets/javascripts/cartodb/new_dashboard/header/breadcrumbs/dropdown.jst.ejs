<ul class="BreadcrumbsDropdown">
  <li class="BreadcrumbsDropdown-item">
    <span class="BreadcrumbsDropdown-icon UserAvatar">
      <img class="UserAvatar--small" src="<%= avatarUrl %>" title="<%= userName %>" alt="<%= userName %>" />
    </span>
    <nav class="BreadcrumbsDropdown-options">
      <a href="<%= mapsUrl %>" class="BreadcrumbsDropdown-optionsItem <%= !isDatasets && !isLocked ? 'is--selected' : '' %>">Your maps</a>
      <a href="<%= datasetsUrl %>" class="BreadcrumbsDropdown-optionsItem <%= isDatasets && !isLocked ? 'is--selected' : '' %>">Your datasets</a>
    </nav>
  </li>
  <li class="BreadcrumbsDropdown-item">
    <i class="iconFont iconFont-Lock BreadcrumbsDropdown-lockIcon BreadcrumbsDropdown-icon"></i>
    <nav class="BreadcrumbsDropdown-options">
      <a href="<%= lockedDatasetsUrl %>" class="BreadcrumbsDropdown-optionsItem <%= isDatasets && isLocked ? 'is--selected' : '' %>">Your locked datasets</a>
      <a href="<%= lockedMapsUrl %>" class="BreadcrumbsDropdown-optionsItem <%= !isDatasets && isLocked ? 'is--selected' : '' %>">Your locked maps</a>
    </nav>
  </li>
</ul
