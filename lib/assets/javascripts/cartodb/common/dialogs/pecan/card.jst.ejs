<li class="GalleryList-item MapsList-item js-card">
  <div class="MapCard">
    <a href="#" class="MapCard-header js-header is-loading">
      <div class="MapCard-loader js-loader"></div>
    </a>
    <div class="MapCard-content MapCard-content--compact">
      <div class="MapCard-contentBody">
        <div class="MapCard-contentBodyRow">
          <h3 class="DefaultTitle">
            <a href="#" class="DefaultTitle-link u-ellipsLongText" ><%= wizard %> on <strong><%= column %></strong></a>
            <span class="NullCount">
              <%= null_count %> null value<%= null_count !== 1 ? 's' : '' %>
            </span>
          </h3>
        </div>
      </div>
    </div>
  </div>
</li>
