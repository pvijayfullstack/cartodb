<div class="MergeDatasets">
  <div class="MergeDatasets-list">
    <div class="List List--selections">
      <div class="List-row is-disabled">
        <div class="List-rowItem List-rowItem--tight">
          <select disabled="disabled">
            <option><%- leftTableName %></option>
          </select>
        </div>
      </div>
      <div class="List-row is-disabled">
        <div class="List-rowItem">
          <div class="List-rowItemContentLeft">
            <div class="RadioButton">
              <button class="RadioButton-input is-checked is-disabled" />
            </div>
            <div>
              <div class="DefaultTitle"><%- leftKeyColumn.get('name') %></div>
              <p class="DefaultParagraph DefaultParagraph--tertiary"><%- leftKeyColumn.get('type') %></p>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="List js-left-columns"></div>
  </div>

  <div class="MergeDatasets-list">
    <% if (rightTableName) { %>
      <div class="List List--selections">
        <div class="List-row is-disabled">
          <div class="List-rowItem List-rowItem--tight">
            <select disabled="disabled">
              <option><%- rightTableName %></option>
            </select>
          </div>
        </div>
        <div class="List-row is-disabled">
          <div class="List-rowItem">
            <div class="List-rowItemContentLeft">
              <div class="RadioButton">
                <button class="RadioButton-input is-checked" />
              </div>
              <div>
                <div class="DefaultTitle"><%- rightKeyColumn.get('name') %></div>
                <p class="DefaultParagraph DefaultParagraph--tertiary"><%- rightKeyColumn.get('type') %></p>
              </div>
            </div>
          </div>
        </div>
      </div>
    <% } else { %>
      <div class="List">
        <div class="List-row">
          <div class="List-rowItem List-rowItem--tight">
            <select class="js-right-tables">
              <option>select dataset</option>
            </select>
          </div>
        </div>
      </div>
      <div class="List">
        <div class="List-row">
          <div class="MergeDatasets-mergeMethodsLegend js-merge-methods-desc"></div>
        </div>
      </div>
    <% } %>

    <% if (rightTableName) { %>
      <div class="List js-right-columns"></div>
      <div class="List js-count-merge-method-info" style="display: none;">
        This will count the number of features in your dataset intersecting the geometries in a second dataset.
      </div>
      <div class="List">
        <div class="List-row TabsPanel js-merge-methods"></div>
      </div>
    <% } %>
  </div>
</div>
