<div class="Dialog-header BackgroundPollingDetails-header">
  <div class="Dialog-headerIcon GeocodingResultDetails-headerIcon GeocodingResultDetails-headerIcon--failed">
    <% if (geometryType && geometryType !== "point") { %>
      <i class="iconFont iconFont-Globe GeocodingResultDetails-globeIcon"></i>
    <% } else { %>
      <div class="GeocodingResultDetails-points"></div>
    <% } %>
  </div>
  <h3 class="Dialog-headerTitle Dialog-headerTitle--error">Georeference Error</h3>
</div>
<div class="Dialog-body BackgroundPollingDetails-body">
  <p class="DefaultParagraph DefaultParagraph--centerAligned">
  <%= errorDescription || "There was a problem georeferencing your data" %>.
  <% if (!customHosted && id) { %>
    Please try again and if the problem persists, <a href="mailto:support@cartodb.com?subject=Geocoding error with id:<%= id %>">contact us</a>
    with the following code: <br/><strong><%= id %></strong>.
  <% } %>
  </p>
</div>
<div class="Dialog-footer BackgroundPollingDetails-footer">
  <button class="Button Button--secondary BackgroundPollingDetails-footerButton cancel">
    <span>close</span>
  </button>
</div>