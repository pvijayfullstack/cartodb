<div class="colors">
  <ul class="colors">
    <% for (var i = 0, l = colors.length; i < l; i++){ %><li><a href="<%= colors[i] %>" style="background:<%= colors[i] %>;"></a></li><% } %>
  </ul>
</div>
<div class="bottom">
  <div class="form_pattern left">
    <form>
      <span class="pattern" style="background:#FF3366"></span>
      <input type="text" value="#FF3366"/>
    </form>
  </div>
  <% if (file_pattern) { %>
    <div class="right">
      <a href="#/choose-asset-pattern" class="asset_pattern">image fill</a>
    </div>
  <% } %>
</div>
