<nav>
  <% _.each( formats, function( format ){ %>
    <a  data-format="<%- format.format %>"
        class="button grey export">
    <%- format.format %>
   </a>
  <% }); %>

</nav>
<div class="hidden generating">
  <p>We are preparing your download. Depending on the size, it could take some time.</p>
</div>

<form class="hack" method="POST" action="<%- url %>">
  <input type="hidden" class="filename" name="filename"></input>
  <input type="hidden" class="q" name="q"></input>
  <input type="hidden" class="format" name="format"></input>
  <input type="hidden" class="api_key" name="api_key"></input>
  <input type="hidden" class="skipfields" name="skipfields" value="the_geom,the_geom_webmercator"></input>
  <input type="hidden" class="dp" name="dp" value="4" disabled="disabled"></input>
</form>
