<div class="mamufas">
  <section class="block modal <%= modal_type %>">
    <a href="#/close" class="close">x</a>
    <div class="head">
      <h3><%= title %></h3>
      <% if (description) { %>
        <p class="margin5"><%= description %></p>
      <% } %>
    </div>
    <div class="content"></div>
    <div class="foot">
      <% if (true) { %>
        <p class="left info">Since you are under the free plan your data cannot be private. Need private data? <a href="<%= 'url?' %>?utm_source=Dashboard_Importing_Table&utm_medium=referral&utm_campaign=Upgrade_from_Dashboard&utm_content=Upgrade%20your%20account">Upgrade your account</a> (14 days free trial)
        </p>
      <% } %>
      <a href="#/<%= ok_title %>" class="right ok <%= ok_button_classes %>"><%= ok_title %></a>
    </div>
  </section>
</div>