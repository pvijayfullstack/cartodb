<div class="mamufas">
  <section class="block modal <%= modal_type %>">
    <a href="#/close" class="close">x</a>
    <div class="head">
      <h3><%= title %></h3>
      <% if (description) { %>
        <p class="margin5"><%= description %></p>
      <% } %>
    </div>
    <div class="content">
      <div class="vis_privacy_selector"></div>
      <div class="org_users_list"></div>
    </div>
    <div class="foot">
      <a href="#/save-settings" class="right ok <%= ok_button_classes %>"><%= ok_title %></a>
    </div>
  </div>
</div>
