<div class="info">
  <% if (avatar_url) { %>
    <img src="<%= avatar_url %>" title="<%- username %>" alt="<%- username %>" />
  <% } %>
  <p class="ellipsis"><%- username %></p>
</div>
<div class="switches">
  <% if (hasPermissions) { %>
    <a href="#/can-write" class="checkbox small light canwrite">
      <span class="check"></span>
      Can write
    </a>
    <a href="#/enabled" class="switch disabled">
      <span class="handle"></span>
    </a>
  <% } else { %>
    no permissions for tables
  <% } %>
</div>
