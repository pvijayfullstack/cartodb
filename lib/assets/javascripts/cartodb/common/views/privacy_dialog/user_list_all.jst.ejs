<div class="info" <% if (!hasPermissions) { %>original-title="You can't share it with the organization because you don't have enough permissions for all the related tables" <% } %>>
  <p class="italic light">Default for organization users</p>
</div>
<div class="switches">
  <% if (hasPermissions) { %>
    <% if (writePermissionEnabled) { %>
      <a href="#/can-write" class="checkbox small light canwrite">
        <span class="check"></span>
        Can write
      </a>
      <span class="separator"></span>
    <% } %>
    <a href="#/enabled" class="switch disabled">
      <span class="handle"></span>
    </a>
  <% } %>
</div>
