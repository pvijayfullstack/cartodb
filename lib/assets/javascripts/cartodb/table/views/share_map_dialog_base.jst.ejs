<div class="mamufas">
  <section class="block modal <%= modal_type %>">
    <a href="#close" class="close">x</a>
    <div class="head">
      <nav>
        <a href="#url" class="strong smaller uppercase">url</a>
        <a href="#embed" class="strong smaller uppercase">embed</a>
        <a href="#api" class="strong smaller uppercase">api</a>
      </nav>

      <h3><%= title %></h3>
      <% if (description) { %>
        <p class="margin5"><%= description || "" %></p>
      <% } %>
    </div>
    <div class="content">
    </div>
    <% if(include_footer) { %>
      <div class="foot">
        <a href="#go" class="right ok <%= ok_button_classes %>"><%= ok_title %></a>
      </div>
    <% } %>
  </div>
</div>
