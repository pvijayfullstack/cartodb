<div class="dialog-content">
  <div class="assets">
    <div class="loader">
      <p>Getting assets...</p>
    </div>
  </div>
  <div class="uploader">
    <ul class="upload-tabs">
      <% if (kind === "marker") { %>
        <li class="upload-tab"><a href="#/simpleicon" class="simpleicon">Simple icons</a></li>
        <li class="upload-tab"><a href="#/pin-maps"   class="pin-maps">Pin icons</a></li>
        <li class="upload-tab"><a href="#/maki"       class="maki">Maki icons</a></li>
        
        <li class="upload-tab right"><a href="#/dropbox"  class="dropbox"><i></i>Dropbox</a></li>
        <li class="upload-tab right"><a href="#/file"     class="file">Upload file</a></li>
        <li class="upload-tab right"><a href="#/assets"   class="assets">Your icons</a></li>
      <% } else { %>
        <li class="upload-tab"><a href="#/assets"   class="assets">Your icons</a></li>
        <li class="upload-tab"><a href="#/file"     class="file">Upload file</a></li>
        <li class="upload-tab"><a href="#/dropbox"  class="dropbox"><i></i>Dropbox</a></li>  
      <% } %>
    </ul>
    <div class="upload-panes"></div>
  </div>
</div>
<div class="upload-progress">
  <span></span>
  <p>Uploading your data</p>
  <a href="#/stop-process" class="stop"></a>
</div>
<div class="hide">
  <form class="dialog-uploader"><input type="file" /></form>
</div>